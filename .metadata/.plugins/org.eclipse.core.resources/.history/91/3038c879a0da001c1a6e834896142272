package com.qa.exercises;

public class Flowcharts {
	
	public void flowChartA(){
	
	
		for(int a=100; a<200; a++) {
		System.out.println(a);
		
		}
	 
	}	

	public void flowChartB() {
		
		
		for (int a=100; a<=200; a++) {
			
			if (a%2==0) {
			System.out.println("-"+ a);
			continue;
			}
			else {
				System.out.println("*"+a);
			}
		 
		
		}
		
		System.out.println("Completed");	
		
	}
	
	public void qThree() {
		
		for (int a=1; a<=10; a++) {
			
			for(int b=1; b<a; b++) {
				System.out.println(a);
			}
			

		}
		
	}
	
	
}
